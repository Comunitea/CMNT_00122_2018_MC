<?xml version="1.0" encoding="utf-8"?>
<odoo noupdate="1">

    <record model="ir.rule" id="scat_student_comp_rule">
        <field name="name">Calendar: multi-company</field>
        <field name="model_id" ref="model_scat_student"/>
        <field name="global" eval="True"/>
